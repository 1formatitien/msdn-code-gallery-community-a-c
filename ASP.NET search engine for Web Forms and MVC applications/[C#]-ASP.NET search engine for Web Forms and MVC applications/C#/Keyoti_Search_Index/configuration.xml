<?xml version="1.0" encoding="utf-8"?>
<KeyotiSearchConfig Version="2012">
  <!--************************************************************  General  -->
  <DataAccessExceptionsThreshold>0</DataAccessExceptionsThreshold>
  <ErrorPageRedir>aspxerrorpath=</ErrorPageRedir>
  <EventHandlerAssemblyPath>
  </EventHandlerAssemblyPath>
  <LogFileMaxSize>2000000</LogFileMaxSize>
  <Logging>False</Logging>
  <MaxResponseHeaderLength>64</MaxResponseHeaderLength>
  <ReaderExceptionsThreshold>2000</ReaderExceptionsThreshold>
  <RequestTimeout>25000</RequestTimeout>
  <UserAgent>Mozilla/4.0 (compatible; MSIE 7.0b; Windows NT 6.0)</UserAgent>
  <!--************************************************************  Import/Crawl  -->
  <CacheCrawlLinks>False</CacheCrawlLinks>
  <CrawlSubdomains>False</CrawlSubdomains>
  <CreateForwardIndex>True</CreateForwardIndex>
  <DbCommandTimeout>30</DbCommandTimeout>
  <DbImportPageSize>500</DbImportPageSize>
  <DefaultPageNames>
    <DefaultPageName>default.aspx</DefaultPageName>
    <DefaultPageName>default.asp</DefaultPageName>
    <DefaultPageName>default.htm</DefaultPageName>
    <DefaultPageName>default.html</DefaultPageName>
    <DefaultPageName>index.html</DefaultPageName>
    <DefaultPageName>index.htm</DefaultPageName>
    <DefaultPageName>issstart.asp</DefaultPageName>
  </DefaultPageNames>
  <IgnoreCommonResourceTypes>True</IgnoreCommonResourceTypes>
  <IgnoreSSLErrors>False</IgnoreSSLErrors>
  <IndexNumbers>True</IndexNumbers>
  <MaximumCrawlDepth>999999</MaximumCrawlDepth>
  <RespectsRobotsMetaTags>True</RespectsRobotsMetaTags>
  <RespectsRobotsTXT>False</RespectsRobotsTXT>
  <URLCaseSensitive>False</URLCaseSensitive>
  <!--************************************************************  Indexer  -->
  <BoostFactorTagName>keyoti_search_weight_boost_factor</BoostFactorTagName>
  <DocumentWorkTimeout>600000</DocumentWorkTimeout>
  <FileTypesSettings>
    <FileType Mime="application/rtf" Extension="rtf" />
    <FileType Mime="application/vnd.ms-word.document.macroEnabled.12" Extension="docm" />
    <FileType Mime="application/vnd.ms-outlook" Extension="msg" />
    <FileType Mime="text/plain" Extension="txt" />
    <FileType Mime="application/zip" Extension="zip" />
    <FileType Mime="text/csv" Extension="csv" />
    <FileType Mime="application/vnd.ms-powerpoint" Extension="ppt" />
    <FileType Mime="application/vnd.ms-excel.sheet.macroEnabled.12" Extension="xlsm" />
    <FileType Mime="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" Extension="xlsx" />
    <FileType Mime="application/vnd.ms-powerpoint.presentation.macroEnabled.12" Extension="pptm" />
    <FileType Mime="text/html" Extension="html" />
    <FileType Mime="text/html" Extension="htm" />
    <FileType Mime="application/pdf" Extension="pdf" />
    <FileType Mime="application/vnd.oasis.opendocument.text" Extension="odt" />
    <FileType Mime="text/xml" Extension="xml" />
    <FileType Mime="application/vnd.oasis.opendocument.spreadsheet" Extension="ods" />
    <FileType Mime="application/vnd.ms-excel" Extension="xls" />
    <FileType Mime="application/vnd.openxmlformats-officedocument.presentationml.presentation" Extension="pptx" />
    <FileType Mime="application/vnd.openxmlformats-officedocument.wordprocessingml.document" Extension="docx" />
    <FileType Mime="application/msword" Extension="doc" />
  </FileTypesSettings>
  <IgnoreBlockBeginPattern>&lt;!--keyoti_search_ignore_begin--&gt;</IgnoreBlockBeginPattern>
  <IgnoreBlockEndPattern>&lt;!--keyoti_search_ignore_end--&gt;</IgnoreBlockEndPattern>
  <IgnoreLastModifiedDate>False</IgnoreLastModifiedDate>
  <IndexedDocumentBufferSize>250</IndexedDocumentBufferSize>
  <IndexingInterval>0</IndexingInterval>
  <MaxDocumentSizeToIndex>30000</MaxDocumentSizeToIndex>
  <ParseMSWordDocUsingIFilter>False</ParseMSWordDocUsingIFilter>
  <ParseRTFDocUsingIFilter>True</ParseRTFDocUsingIFilter>
  <PdfDocumentTitleSource>Filename</PdfDocumentTitleSource>
  <ProxyAddress>
  </ProxyAddress>
  <ProxyPort>80</ProxyPort>
  <ReadDocumentInNewThread>True</ReadDocumentInNewThread>
  <RequestThrottle>0</RequestThrottle>
  <UseFileSizeToIdentifyChange>True</UseFileSizeToIdentifyChange>
  <WebsiteCredentials />
  <WordBreakingCharacters>*=()[]{}&lt;&gt;"/\-</WordBreakingCharacters>
  <WordNonBreakingCharacters>,.!@#$%^&amp;+|;:'?~`’”_</WordNonBreakingCharacters>
  <WordWeightFactorBody>10</WordWeightFactorBody>
  <WordWeightFactorTitle>100</WordWeightFactorTitle>
  <WordWeightFactorUrl>90</WordWeightFactorUrl>
  <!--************************************************************  Search  -->
  <CustomDataFilterLiveDataMaximum>100</CustomDataFilterLiveDataMaximum>
  <ExpressionOperatorsCollection>
    <ExpressionOperator>AND=AND, OR=OR</ExpressionOperator>
  </ExpressionOperatorsCollection>
  <IgnoreDiacritics>True</IgnoreDiacritics>
  <ImpliedLogicOperator>And</ImpliedLogicOperator>
  <LemmaLanguage>English</LemmaLanguage>
  <LemmaSearchEnabled>True</LemmaSearchEnabled>
  <LemmaWeightFactor>0.5</LemmaWeightFactor>
  <ResultSummaryType>Dynamic</ResultSummaryType>
  <SummaryTextSplitter>...</SummaryTextSplitter>
  <SummaryWordLength>25</SummaryWordLength>
  <WordsAroundNumber>4</WordsAroundNumber>
  <!--************************************************************  Windows Service  -->
  <ServiceRebuildFrequency>10000</ServiceRebuildFrequency>
  <ServiceRebuildHour>1</ServiceRebuildHour>
  <ServiceWillOptimizeIndex>True</ServiceWillOptimizeIndex>
  <ServiceWillSendEmailLog>False</ServiceWillSendEmailLog>
  <!--************************************************************  Windows Service Email Settings  -->
  <SmtpEmailFrom>
  </SmtpEmailFrom>
  <SmtpEmailTo>
  </SmtpEmailTo>
  <SmtpMailServer>
  </SmtpMailServer>
  <SmtpPassword>
  </SmtpPassword>
  <SmtpServerPort>25</SmtpServerPort>
  <SmtpUsername>
  </SmtpUsername>
</KeyotiSearchConfig>